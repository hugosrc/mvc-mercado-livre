{
  "compilerOptions": {
    "target": "es5",                          
    "module": "commonjs",
    "allowJs": true,           
    "outDir": "./dist",                        
    "rootDir": "./src",          
    "strict": true,       
    "esModuleInterop": true,   
    "skipLibCheck": true,                     
    "forceConsistentCasingInFileNames": true  
  },
  "include": [
    "src"
  ]
}
